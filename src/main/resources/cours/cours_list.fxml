<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.controller.cours.CoursListController">
    <children>
        <VBox spacing="10" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.bottomAnchor="10">
            <TableView fx:id="coursTable" prefHeight="300">
                <columns>
                    <TableColumn fx:id="titleColumn" text="Titre" />
                    <TableColumn fx:id="descColumn" text="Description" />
                    <TableColumn fx:id="priceColumn" text="Prix" />
                    <TableColumn fx:id="typeColumn" text="Type" />
                </columns>
            </TableView>
            <Button fx:id="modifyBtn" text="Modifier" disable="true" />
        </VBox>
    </children>
</AnchorPane>
